<?xml version="1.0" encoding="UTF-8"?>
<sm:binaryPrim xmlns:sc="http://www.utc.fr/ics/scenari/v3/core" xmlns:sm="http://www.utc.fr/ics/scenari/v3/modeling" name="Classeur OpenDocument" category="Ressources">
	<sm:help quickHelp=" Ressource au format Open Document Spreadsheet. "/>
	<sm:identification extensions="ods"/>
	<sm:structure type="application/vnd.oasis.opendocument.spreadsheet"/>
	<sm:storage>
		<sm:glimpseImage>
			<sm:fromBinary>
				<sm:freeTransform code="ods2png" options="maxWidth=300px"/>
			</sm:fromBinary>
		</sm:glimpseImage>
	</sm:storage>
	<sm:authoring>
		<sm:previewBlock>
			<sm:imageViewer>
				<sm:fromBinary>
					<sm:freeTransform code="ods2png"/>
				</sm:fromBinary>
			</sm:imageViewer>
		</sm:previewBlock>
		<sm:icon sc:refUri="/binaries/ods/_res/ods.png"/>
		<sm:itemNameCreator>
			<sm:fixedString value="res"/>
			<sm:counter optional="true"/>
		</sm:itemNameCreator>
		<sm:emptyFile sc:refUri="/binaries/ods/_res/empty.ods"/>
	</sm:authoring>
</sm:binaryPrim>